{
  "extends": "next/core-web-vitals",
  "rules": {
    "comma-dangle": "error",
    "eol-last": ["error", "always"],
    "max-len": ["error", { "code": 80, "comments": 80 }],
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    "no-trailing-spaces": "error",
    "object-curly-spacing": ["warn", "always"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
